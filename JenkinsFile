Jenkinsfile (Declarative Pipeline)
pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
               echo "Deploy"
            }
        }
        stage('Test') {
            steps {
               echo "Deploy"
            }
        }
        stage('Deploy') {
            steps {
                echo "Deploy"

            }
        }
    }
}
