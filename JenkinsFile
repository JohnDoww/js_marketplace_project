Jenkinsfile (Declarative Pipeline)
pipeline {
    agent any
    stages {
        stage('Install dependencies') {
            steps {
               echo "Build"
            }
        }
        stage('Test run') {
            steps {
               echo "Test"
            }
        }
        stage('Allure report') {
            steps {
                echo "Deploy"

            }
        }
    }
}
